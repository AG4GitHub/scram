define-basic-event :: define-basic-event
define-house-event :: define-house-event

-->

<opsa-mef>
  <define-fault-tree name="Two Trains">
    <define-gate name="TopEvent">
      <and>
        <gate name="TrainOne"/>
        <event name="TrainTwo"/>
        <gate name="TrainThree"/>
        <event name="ValveOne"/>
        <event name="PumpOne"/>
      </and>
    </define-gate>
    <define-gate name="TrainOne">
      <or>
        <basic-event name="ValveOne"/>
        <house-event name="PumpOne"/>
        <gate name="TrainTwo"/>
      </or>
    </define-gate>
    <define-gate name="TrainTwo">
      <or>
        <event name="TrainThree"/>
        <basic-event name="ValveTwo"/>
        <event name="PumpTwo"/>
        <event name="ValveOne"/>
        <event name="PumpOne"/>
      </or>
    </define-gate>
    <define-gate name="TrainThree">
      <or>
        <basic-event name="ValveTwo"/>
        <event name="PumpTwo"/>
        <event name="ValveOne"/>
        <event name="PumpOne"/>
      </or>
    </define-gate>
    <define-basic-event name="ValveOne">
      <float value="0.4"/>
    </define-basic-event>
    <define-basic-event name="ValveTwo">
      <float value="0.5"/>
    </define-basic-event>
    <define-house-event name="PumpOne">
      <constant value="true"/>
    </define-house-event>
    <define-basic-event name="PumpTwo">
      <float value="0.7"/>
    </define-basic-event>
  </define-fault-tree>
</opsa-mef>
