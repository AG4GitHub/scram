# This is an input file for a lift door fault tree. The example is taken from
# OpenFTA for benchmarking.

{
    parent      None
    id          LiftDoor  # Lift door fails
    type        OR
}

# Subtree One
{
    parent      LiftDoor
    id          LiftStartsDoorOpen
    type        OR
}

{
    parent      LiftStartsDoorOpen
    id          LMD_1  # Lift motion detector fails
    type        Basic
}

{
    parent      LiftStartsDoorOpen
    id          DPD_1  # Door position detector fails
    type        Basic
}

{
    parent      LiftStartsDoorOpen
    id          AND_1
    type        AND
}

{
    parent      AND_1
    id          DM_1  # Door motor fails
    type        Basic
}

{
    parent      AND_1
    id          DPD_1  # Door position detector fails
    type        Basic
}

{
    parent      LiftStartsDoorOpen
    id          DoorMotorSlow  # Door motor is too slow
    type        OR
}

{
    parent      DoorMotorSlow
    id          PS_1  # Power supply fails
    type        Basic
}

{
    parent      DoorMotorSlow
    id          DoorHindered
    type        OR
}

{
    parent      DoorHindered
    id          DOD_1  # Door obsticle detector fails
    type        Basic
}

{
    parent      DoorHindered
    id          D_1  # Door failure
    type        Undeveloped
}

# Subtree Two
{
    parent      LiftDoor
    id          DoorOpensInMotion  # Lift door opens when in motion
    type        OR
}

{
    parent      DoorOpensInMotion
    id          DMS_2  # Door motor stop fails
    type        Basic
}

{
    parent      DoorOpensInMotion
    id          AND_2
    type        AND
}

{
    parent      AND_2
    id          UncommandedDoorOpen
    type        OR
}

{
    parent      UncommandedDoorOpen
    id          W_1  # Wiring short out
    type        Undeveloped
}

{
    parent      UncommandedDoorOpen
    id          CP_1  # Control panel fails
    type        Basic
}

{
    parent      UncommandedDoorOpen
    id          DMS_2  # Door motor stop fails
    type        Basic
}

{
    parent      AND_2
    id          MotionDetector  # Lift motion detector fails
    type        AND
}

{
    parent      MotionDetector
    id          LMD_1  # Lift motion detector fails
    type        Basic
}

{
    parent      MotionDetector
    id          LMD_1_1  # Lift motion detector 2 fails
    type        Basic
}

{
    parent      DoorOpensInMotion
    id          AND_3
    type        AND
}

{
    parent      AND_3
    id          CP_1  # Commonded door oopen. Control panel fails.
    type        Basic
}

{
    parent      AND_3
    id          MotionDetector_R  # This should be a transfer in
    type        AND
}

{
    parent      MotionDetector_R
    id          LMD_1  # Lift motion detector fails
    type        Basic
}

{
    parent      MotionDetector_R
    id          LMD_1_1
    type        Basic
}

# Subtree Three
{
    parent      LiftDoor
    id          DoorRefuses  # Door refuses to open when lift stopped.
    type        OR
}

{
    parent      DoorRefuses
    id          CP_1  # Control panel fails
    type        Basic
}

{
    parent      DoorRefuses
    id          LMD_2
    type        Basic
}

{
    parent      DoorRefuses
    id          LPD_1
    type        Basic
}

{
    parent      DoorRefuses
    id          DoorMotor
    type        OR
}

{
    parent      DoorMotor
    id          DM_1  # Door motor fails
    type        Basic
}

{
    parent      DoorMotor
    id          PS_1
    type        Basic
}

# Subtree Four
{
    parent      LiftDoor
    id          OR_1
    type        OR
}

{
    parent      OR_1
    id          DOD_1  # Obsticle detector fails
    type        Basic
}

{
    parent      OR_1
    id          DOD_2  # Incorrect door obsticle detector fitted
    type        Basic
}

{
    parent      OR_1
    id          DMS_1  # Door motor stop fails
    type        Basic
}
