    type        Undeveloped
}

{
    parent      E3
    id          E6
    type        OR
}

{
    parent      E6
    id          KT2inc
    type        Basic
}

{
    parent      E6
    id          T3inc
    type        Undeveloped
}

{
    parent      E3
    id          E7
    type        OR
}

{
    parent      E7
