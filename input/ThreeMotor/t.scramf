# This is a transfer sub-tree for Three Motor system taken from OpenFTA.

{
    parent      Any
    id          t.scramf
    type        TransferOut
}

{
    parent      t.scramf
    id          E1  # K1 relay contacts fail to open when K3 contacts closed
    type        OR
}

{
    parent      E1
