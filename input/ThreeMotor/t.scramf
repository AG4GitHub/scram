    type        Basic
}

{
    parent      E4
    id          T1inc  # Reset signal inadvertently applied
    type        Undeveloped
}

{
    parent      E2
    id          E3  # EMF to K1 coil through timer circuit when K3 contacts
                    # closed for t > 60 sec
    type        AND
}

{
    parent      E3
    id          E5  # K1 timer contacts fail to open when K3 closed
    type        OR
}

{
    parent      E5
